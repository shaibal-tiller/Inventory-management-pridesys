{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://4.213.57.100:3100/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "upgrade-insecure-requests"
        }
      ]
    }
  ]
}
